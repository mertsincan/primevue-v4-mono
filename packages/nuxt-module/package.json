{
    "name": "@primevue-lab/nuxt-module",
    "version": "4.0.0-beta.4",
    "description": "Nuxt module for primevue-lab",
    "keywords": [
        "vue",
        "vue.js",
        "vue3",
        "nuxt",
        "nuxt3",
        "primevue-lab",
        "primevue-lab nuxt module",
        "primevue-lab nuxt3",
        "primevue-lab 4"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/primefaces/primevue-lab-nuxt-module.git",
        "directory": "packages/nuxt-module"
    },
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "main": "./src/module.ts",
    "types": "./src/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepack": "nuxt-module-build",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "npm run format && npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
        "lint": "eslint .",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "format": "prettier --write \"**/*.{vue,ts,d.ts}\" --cache"
    },
    "dependencies": {
        "@nuxt/kit": "^3.7.3",
        "pathe": "^1.1.2",
        "primevue-lab": "workspace:*"
    },
    "devDependencies": {
        "@types/node": "^18.17.17",
        "@nuxt/devtools": "^0.8.5",
        "@nuxt/eslint-config": "^0.2.0",
        "@nuxt/module-builder": "^0.5.1",
        "@nuxt/schema": "^3.7.3",
        "@nuxt/test-utils": "^3.7.3",
        "changelogen": "^0.5.5",
        "eslint": "^8.49.0",
        "nitropack": "^2.6.3",
        "nuxt": "3.3.2",
        "prettier": "2.7.1",
        "vitest": "^1.6.0"
    },
    "publishConfig": {
        "main": "./dist/module.cjs",
        "types": "./dist/types.d.ts",
        "exports": {
            ".": {
                "types": "./dist/types.d.ts",
                "import": "./dist/module.mjs",
                "require": "./dist/module.cjs"
            }
        },
        "access": "public"
    }
}
